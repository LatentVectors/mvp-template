{
  "deno.enablePaths": [
    "./supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "editor.rulers": [
    120
  ],
  "files.exclude": {
    "**/node_modules": true,
    "**/.venv": true,
    "**/venv": true,
    "**/__pycache__": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/.tox": true,
    "**/.ipynb_checkpoints": true,
    "**/.cache": true,
    "**/.DS_Store": true,
    "**/.idea": true,
    "**/.next": true,
    "**/.nuxt": true,
    "**/.expo": true,
    "**/.svelte-kit": true,
    "**/dist": true,
    "**/build": true,
    "**/out": true,
    "**/coverage": true,
    "**/.turbo": true,
    "**/.parcel-cache": true,
    "**/.swc": true,
    "**/.vite": true,
    "**/.rollup.cache": true,
    "**/.pnpm-store": true,
    "**/.yarn/cache": true,
    "**/.vercel": true,
    "**/supabase/.branches": true,
    "**/supabase/.temp": true
  },
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/.venv/**": true,
    "**/venv/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/out/**": true,
    "**/coverage/**": true,
    "**/.next/**": true,
    "**/.turbo/**": true,
    "**/.parcel-cache/**": true,
    "**/.vite/**": true,
    "**/.svelte-kit/**": true,
    "**/.swc/**": true,
    "**/.pnpm-store/**": true,
    "**/.yarn/cache/**": true,
    "**/supabase/.temp/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.venv": true,
    "**/venv": true,
    "**/__pycache__": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/.tox": true,
    "**/.ipynb_checkpoints": true,
    "**/.cache": true,
    "**/.DS_Store": true,
    "**/.idea": true,
    "**/.next": true,
    "**/.nuxt": true,
    "**/.expo": true,
    "**/.svelte-kit": true,
    "**/dist": true,
    "**/build": true,
    "**/out": true,
    "**/coverage": true,
    "**/.turbo": true,
    "**/.parcel-cache": true,
    "**/.swc": true,
    "**/.vite": true,
    "**/.rollup.cache": true,
    "**/.pnpm-store": true,
    "**/.yarn/cache": true,
    "**/.vercel": true,
    "**/*.log": true,
    "**/.env": true,
    "**/.env.*": true,
    "**/supabase/.branches": true,
    "**/supabase/.temp": true
  },
  "explorer.excludeGitIgnore": true,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.ts": "$(capture).test.ts, $(capture).spec.ts, $(capture).stories.ts, $(capture).stories.mdx, $(capture).snap, $(capture).map, $(capture).css, $(capture).scss, $(capture).module.css, $(capture).module.scss",
    "*.tsx": "$(capture).test.tsx, $(capture).spec.tsx, $(capture).stories.tsx, $(capture).stories.mdx, $(capture).snap, $(capture).map, $(capture).css, $(capture).scss, $(capture).module.css, $(capture).module.scss",
    "*.js": "$(capture).test.js, $(capture).spec.js, $(capture).stories.js, $(capture).snap, $(capture).map, $(capture).css, $(capture).scss",
    "*.jsx": "$(capture).test.jsx, $(capture).spec.jsx, $(capture).stories.jsx, $(capture).snap, $(capture).map, $(capture).css, $(capture).scss"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "search.useIgnoreFiles": true,
  "search.useGlobalIgnoreFiles": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "files.associations": {
    "*.mdx": "mdx"
  },
  "diffEditor.ignoreTrimWhitespace": true
}