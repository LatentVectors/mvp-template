{
  "name": "@repo/interfaces-storage",
  "version": "1.0.0",
  "description": "Storage port and adapters (Supabase) for the MVP template",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "echo 'No linting configured for interfaces-storage package'",
    "test": "vitest run"
  },
  "author": "LatentVectors",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.7.2",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4"
  }
}
