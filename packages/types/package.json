{
  "name": "@repo/types",
  "version": "1.0.0",
  "description": "Shared TypeScript types for the MVP template monorepo",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./database": {
      "types": "./dist/database.d.ts",
      "import": "./dist/database.js",
      "require": "./dist/database.js"
    },
    "./supabase": {
      "types": "./dist/supabase.d.ts",
      "import": "./dist/supabase.js",
      "require": "./dist/supabase.js"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "echo 'No linting configured for types package'",
    "test": "echo 'No tests configured for types package'"
  },
  "keywords": [
    "typescript",
    "types",
    "shared",
    "database",
    "supabase"
  ],
  "author": "LatentVectors",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.0.0"
  }
}
